<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="sungil.mymovie2">

	<uses-permission android:name="android.permission.INTERNET" />

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/Theme.MyMovie2">
		<activity
			android:name=".MovieActivity"
			android:exported="false"
			android:label="@string/str_movie"/>
		<activity
			android:name=".WebviewActivity"
			android:exported="false"
			android:label="@string/str_web"/>
		<activity
			android:name=".RecommendMovieActivity"
			android:exported="false"
			android:label="@string/str_recommend_movie"/>
		<activity
			android:name=".WishMovieActivity"
			android:exported="false"
			android:label="@string/str_wish_movie"/>
		<activity
			android:name=".FavoriteMovieActivity"
			android:exported="false"
			android:label="@string/str_favorite_movie"/>
		<activity
			android:name=".CurrentMovieActivity"
			android:exported="false"
			android:label="@string/str_current_movie"/>
		<activity
			android:name=".AllMovieActivity"
			android:exported="false"
			android:label="@string/str_all_movie"/>
		<activity
			android:name=".MainActivity"
			android:exported="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>

</manifest>